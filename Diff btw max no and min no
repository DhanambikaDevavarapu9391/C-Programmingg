//max and min  numbers in the array//
#include<stdio.h>
main()
{
	int a[10],i,n,min,max;
	printf("Enter the size of the array:");
	scanf("%d",&n);
	printf("Enter the array elements:");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	min=max=a[0];
	for(i=1;i<n;i++)
	{
		if(a[i]>max)
		max=a[i];
		if(a[i]<min)
		min=a[i];
		
	}
	printf("%d %d",max,min);
	
}
