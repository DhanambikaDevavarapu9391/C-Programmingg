#include <stdio.h>
int main() {
  int l, b;
  printf("Enter the length of the rectangle: ");
  scanf("%d", &l);
  printf("Enter the breadth of the rectangle: ");
  scanf("%d", &b);
  int result = area(l, b);
  printf("The area of the rectangle is %d\n", result);
  return 0;
}
int area(int length, int breadth) {
  return length * breadth;
}
